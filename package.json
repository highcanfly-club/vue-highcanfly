    "@popperjs/core": "2.11.6",
    "@sanity/client": "^3.4.1",
    "@sanity/image-url": "^1.0.1",
    "@vuepic/vue-datepicker": "^3.5.2",
    "@vueuse/core": "^9.3.0",
    "basiclightbox": "^5.0.4",
    "buffer": "^6.0.3",
    "cesium": "^1.98.1",
    "chart.js": "^3.9.1",
    "crypto-es": "^1.2.7",
    "dexie": "^4.0.0-alpha.4",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "jose": "^4.10.0",
    "leaflet": "^1.9.2",
    "path-browserify": "^1.0.1",
    "qrcode.vue": "^3.3.3",
    "sanity-blocks-vue-component": "^1.0.1",
    "vue": "^3.2.40",
    "vue-agile": "^2.0.0",
    "vue-instantsearch": "~4.6.0",
    "vue-router": "^4.1.5",
    "vue3-openlayers": "^0.1.70",
    "walkjs": "^3.2.4"
